{
  "name": "fixit-app",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "dev": "next dev --turbopack -H 0.0.0.0",
    "build": "next build",
    "build:check": "tsc --noEmit",
    "start": "next start",
    "tunnel": "bun scripts/tunnel-untun.ts",
    "tunnel:serveo": "bun scripts/tunnel.ts",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "test": "for f in $(find src/tests/unit -name '*.test.ts' -o -name '*.test.tsx'); do bun test \"$f\" || exit 1; done",
    "test:watch": "bun test src --watch",
    "test:api": "for f in src/tests/unit/api/*.test.ts; do bun test \"$f\" || exit 1; done",
    "test:actions": "for f in src/tests/unit/actions/*.test.ts; do bun test \"$f\" || exit 1; done",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:seed": "bun run src/db/seed.ts",
    "minio:setup": "bun run scripts/setup-minio.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.712.0",
    "@aws-sdk/s3-request-presigner": "^3.712.0",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@react-pdf/renderer": "^4.3.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "drizzle-orm": "^0.45.1",
    "framer-motion": "^12.23.26",
    "html5-qrcode": "^2.3.8",
    "jose": "^6.1.3",
    "lucide-react": "^0.468.0",
    "next": "^15.1.0",
    "pino": "^10.1.0",
    "postgres": "^3.4.7",
    "qrcode": "^1.5.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "recharts": "^3.6.0",
    "tailwind-merge": "^2.6.0",
    "uuidv7": "^1.1.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@ducanh2912/next-pwa": "^10.2.9",
    "@happy-dom/global-registrator": "^20.0.11",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.0.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/bun": "^1.3.5",
    "@types/minimatch": "^5.1.2",
    "@types/node": "^22.10.2",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "drizzle-kit": "^0.30.1",
    "happy-dom": "^20.0.11",
    "pino-pretty": "^13.1.3",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.7.2"
  }
}
